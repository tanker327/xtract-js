{
  "name": "@tanker327/xtract",
  "version": "0.2.0",
  "description": "A TypeScript library for extracting Twitter/X post data with automatic URL expansion and rich content support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "tsup.config.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "format": "prettier --write src",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "twitter",
    "x",
    "extraction",
    "scraper",
    "api",
    "typescript",
    "url-expansion",
    "social-media",
    "zod"
  ],
  "author": "Eric Wu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanker327/x-tract.git"
  },
  "homepage": "https://github.com/tanker327/x-tract#readme",
  "bugs": {
    "url": "https://github.com/tanker327/x-tract/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "zod": "^4.1.13"
  }
}
